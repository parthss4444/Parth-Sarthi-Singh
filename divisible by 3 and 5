import java.util.Scanner;

class test2{
    public static void main(String[] args){
        Scanner Sc=new Scanner(System.in);
        int n=Sc.nextInt();
        if(n%3==0&&n%5==0) {
            System.out.println("number divisible by 3 and 5");
        }
        else{
            System.out.println(("number not divisible by 3 and 5"));
        }
    }
}
