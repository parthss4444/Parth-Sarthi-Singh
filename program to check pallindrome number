import java.util.Scanner;

import java.util.*;
class pallindrome{
    public static void main(String[] args) {
        int rem,rev=0;
        Scanner Sc = new Scanner(System.in);
        int n = Sc.nextInt();
        int m=n;
        while (n > 0) {
            rem = n % 10;
            rev = rev * 10 + rem;
            n = n / 10;
        }
        if (rev == m) {
            System.out.println("pallindrome number");
        }
else{
                System.out.println("Not a pallindrome number");
            }
        }
    }
