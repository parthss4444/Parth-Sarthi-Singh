import java.util.Scanner;

class reverse {
    public static void main(String[] args) {
        int rev = 0, rem;
        Scanner Sc = new Scanner(System.in);
        int n = Sc.nextInt();
        while (n > 0) {
            rem = n % 10;
            rev = rev * 10 + rem;
            n = n / 10;
        }
        System.out.println(rev);
    }
}
